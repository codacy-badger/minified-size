{
  "name": "minified-size",
  "version": "0.0.1",
  "description": "Estimates the size of minified JavaScript files.",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/minified-size/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/minified-size#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prantlf/minified-size.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/minified-size/issues"
  },
  "engines": {
    "node": ">=8"
  },
  "main": "lib/index.js",
  "bin": {
    "minified-size": "./bin/minified-size"
  },
  "scripts": {
    "lint": "standard bin/minified-size lib/*.js test/*.js",
    "check": "tap test/*.js --100",
    "test": "npm run lint && npm run check",
    "coverage": "test `node --version | cut -c 2` -eq 8 && npm run check -- --cov --coverage-report=lcov --no-browser"
  },
  "dependencies": {
    "commander": "^2.17.1",
    "pretty-bytes": "^5.1.0",
    "sprintf-js": "^1.1.1",
    "terser": "^3.8.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "standard": "^12.0.1",
    "tap": "^12.0.1"
  },
  "keywords": [
    "estimate",
    "minified",
    "script",
    "file",
    "size"
  ]
}
